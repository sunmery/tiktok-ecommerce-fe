{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{ts,tsx,js,jsx,cjs,mjs,md}": "eslint --fix",
    "*.{css,scss,sass}": "stylelint --fix",
    "*.{ts,tsx,js,jsx,cjs,mjs}": "prettier --write"
  },
  "scripts": {
    "prepare": "cd .. && husky ./frontend/.husky || true",
    "prettier:check": "prettier --check src/**/**{.cjs,.mjs,.ts,.tsx,.html,.md}",
    "prettier:write": "prettier --write src/**/**{.cjs,.mjs,.ts,.tsx,.html,.md}",
    "lint": "lint-staged",
    "oxlint": "oxlint",
    "eslint": "ESLINT_USE_FLAT_CONFIG=false eslint -c .eslintrc.yml **/*.{ts,tsx,md}",
    "eslint:fix": "eslint --cache --max-warnings 0 src/**/*{.ts,.tsx} --fix",
    "stylelint": "stylelint **/**.{css,scss,sass}",
    "stylelint:fix": "stylelint **/**.{css,scss,sass} --fix",
    "dev": "vite",
    "dev:prod": "vite --mode production",
    "build": "vite build",
    "preview": "vite preview --port 8080",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/inter": "^5.1.0",
    "@mui/icons-material": "^6.1.1",
    "@mui/joy": "5.0.0-beta.48",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-router": "^1.58.12",
    "@tanstack/router-devtools": "^1.58.12",
    "@vitejs/plugin-react": "^4.3.1",
    "casdoor-js-sdk": "^0.16.0",
    "casdoor-react-sdk": "^1.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "valtio": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/preset-react": "^7.24.7",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/prompt-cli": "^19.5.0",
    "@eslint/js": "^8.57.1",
    "@tanstack/eslint-plugin-query": "^5.58.1",
    "@tanstack/router-plugin": "^1.58.12",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "commitlint": "^19.5.0",
    "eslint": "^8.57.1",
    "eslint-config-alloy": "^5.1.2",
    "eslint-plugin-jsdoc": "^48.11.0",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "oxlint": "^0.2.18",
    "prettier": "3.2.5",
    "stylelint": "^16.9.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  }
}
