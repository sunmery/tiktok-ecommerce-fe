#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sunmery/golang/src/01/11/frontend/node_modules/.pnpm/@commitlint+prompt-cli@19.7.0_@types+node@22.12.0_typescript@5.7.3/node_modules/@commitlint/prompt-cli/node_modules:/Users/sunmery/golang/src/01/11/frontend/node_modules/.pnpm/@commitlint+prompt-cli@19.7.0_@types+node@22.12.0_typescript@5.7.3/node_modules/@commitlint/node_modules:/Users/sunmery/golang/src/01/11/frontend/node_modules/.pnpm/@commitlint+prompt-cli@19.7.0_@types+node@22.12.0_typescript@5.7.3/node_modules:/Users/sunmery/golang/src/01/11/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sunmery/golang/src/01/11/frontend/node_modules/.pnpm/@commitlint+prompt-cli@19.7.0_@types+node@22.12.0_typescript@5.7.3/node_modules/@commitlint/prompt-cli/node_modules:/Users/sunmery/golang/src/01/11/frontend/node_modules/.pnpm/@commitlint+prompt-cli@19.7.0_@types+node@22.12.0_typescript@5.7.3/node_modules/@commitlint/node_modules:/Users/sunmery/golang/src/01/11/frontend/node_modules/.pnpm/@commitlint+prompt-cli@19.7.0_@types+node@22.12.0_typescript@5.7.3/node_modules:/Users/sunmery/golang/src/01/11/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@commitlint/prompt-cli/cli.js" "$@"
else
  exec node  "$basedir/../@commitlint/prompt-cli/cli.js" "$@"
fi
